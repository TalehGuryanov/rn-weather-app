{
  "expo": {
    "name": "Weather app",
    "description": "The weather app is a simple app that shows the weather in your city.",
    "owner": "taleh",
    "slug": "wheather-app",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/TalehGuryanov/rn-wheather-app",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/weather-icon-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/weather-icon-iphone.png",
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.taleh.weatherApp",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses the location to show the weather in your city",
        "NSLocationAlwaysUsageDescription": "This app uses the location to show the weather in your city",
        "NSLocationWhenInUseUsageDescription": "This app uses the location to show the weather in your city"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/weather-icon-android.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.taleh.weatherApp",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "e2992b7d-80db-408b-9b44-f57f887408ef"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e2992b7d-80db-408b-9b44-f57f887408ef"
    }
  }
}
